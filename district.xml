<odoo>
    <record id="sk_district_tree_view" model="ir.ui.view">
        <field name="name">sk.district</field>
        <field name="model">sk_district</field>   
        <field name="arch" type="xml">
            <tree>
              <field name="name"/>
            </tree>
        </field>
    </record>    
    
    <record id="sk_district_form_view" model="ir.ui.view">
        <field name="name">sk.district</field>
        <field name="model">sk_district</field>   
        <field name="arch" type="xml">
            <form>
                <group>  
               <group>  
                <field name="name"/>
                </group>
               </group> 
            </form>
        </field>
    </record>    
    
<record id="sk_district_action" model="ir.actions.act_window">
        <field name="name">District</field>
        <field name="res_model">sk_district</field>   
        <field name="view_mode">tree,form</field>
    </record>    
     
    

    
</odoo>