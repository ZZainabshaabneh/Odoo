<odoo>
    <record id="sk_city_tree_view" model="ir.ui.view">
        <field name="name">sk.city</field>
        <field name="model">sk_city</field>   
        <field name="arch" type="xml">
            <tree>
              <field name="name"/>
              <field name="district"/>
            </tree>
        </field>
    </record>    
    
    <record id="sk_city_form_view" model="ir.ui.view">
        <field name="name">sk.city</field>
        <field name="model">sk_city</field>   
        <field name="arch" type="xml">
            <form>
                <group>  
               <group>  
                <field name="name"/>
               <field name="district"/>
                   
                </group>
               </group> 
            </form>
        </field>
    </record>    
    
<record id="sk_city_action" model="ir.actions.act_window">
        <field name="name">City</field>
        <field name="res_model">sk_city</field>   
        <field name="view_mode">tree,form</field>
    </record>    
     
    

    
</odoo>